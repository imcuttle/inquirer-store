{
  "name": "inquirer-store",
  "version": "1.0.1",
  "main": "index.js",
  "description": "Make inquirer's answers persistence even be aborted halfway",
  "author": "imcuttle <moyuyc95@gmail.com>",
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm test",
    "doc": "documentation --github --markdown-toc=false --config=documentation.yml readme index.js Store.js FileStore.js -a public -s \"API\" && git add README.md",
    "version": "npm run doc && npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=6"
  },
  "files": [
    "lib",
    "src",
    "index.js",
    "FileStore.js",
    "Store.js"
  ],
  "keywords": [
    "imcuttle",
    "inquirer-store",
    "inquirer",
    "store",
    "persistence"
  ],
  "license": "MIT",
  "repository": "imcuttle/inquirer-store",
  "jest": {
    "collectCoverageFrom": [
      "index.js",
      "Store.js",
      "FileStore.js"
    ],
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@moyuyc/inquirer-autocomplete-prompt": "^1.0.4",
    "@types/jest": "^23.3.5",
    "conventional-changelog-cli": "^2.0.5",
    "documentation": "^8.1.2",
    "husky": "^1.1.1",
    "inquirer": "^6.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "sinon": "^6.3.5"
  },
  "dependencies": {
    "lodash.toarray": "^4.4.0",
    "mkdirp": "^0.5.1",
    "pify": "^4.0.0"
  }
}
